<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:catalogVersion="561"
        cq:template="/libs/commerce/templates/catalog"
        jcr:language="en"
        jcr:mixinTypes="[mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Master Catalog"
        sling:resourceType="commerce/components/catalog">
        <target
            jcr:primaryType="nt:unstructured"
            coverImage="/content/dam/we-retail/products/Product_catalog.jpg"/>
        <templates
            jcr:primaryType="nt:unstructured"
            catalog="/content/catalogs/we-retail/en/template-pages/catalog-page-template"
            product="/content/catalogs/we-retail/en/template-pages/product-page-template"
            section="/content/catalogs/we-retail/en/template-pages/section-page-template"
            proxyProductPages="false"/>
        <filter
            jcr:primaryType="nt:unstructured"
            basePath="/etc/commerce/products/we-retail"
            searchType="simple"/>
        <cq:CatalogSyncConfig jcr:primaryType="cq:CatalogSyncConfig">
            <catalog
                cq:catalogPath="/content/we-retail/language-masters/en/products"
                cq:rolloutConfigs="[/etc/msm/rolloutconfigs/catalogpagecontent,/etc/msm/rolloutconfigs/catalogrollouthooks]"
                jcr:primaryType="nt:unstructured"/>
        </cq:CatalogSyncConfig>
    </jcr:content>
    <women/>
    <men/>
    <equipment/>
</jcr:root>
